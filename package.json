{
  "name": "webpack-hmr-template",
  "version": "1.0.1",
  "description": "My own template for lazy man",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "start:hot": "better-npm-run start:hot"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --content-base=./src/dest --inline --progress",
      "env": {
        "BABEL_ENV": "development"
      }
    },
    "start:hot": {
      "command": "webpack-dev-server --content-base=./src/dest --inline --hot --progress",
      "env": {
        "BABEL_ENV": "development:hot"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joyfeel/webpack-hmr-template.git"
  },
  "keywords": [],
  "author": "Joey Chen <joybee210@gmail.com> (https://github.com/joyfeel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joyfeel/webpack-hmr-template/issues"
  },
  "homepage": "https://github.com/joyfeel/webpack-hmr-template#readme",
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.6.5",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "better-npm-run": "0.0.8",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "node-sass": "3.4.2",
    "react-transform-hmr": "1.0.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "react": "0.14.7",
    "react-dom": "0.14.7"
  }
}
