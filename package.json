{
  "name": "webpack-hmr-template",
  "version": "1.0.0",
  "description": "My own template for lazy man",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base=./src/dest --inline --hot --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-hmr",
                  "imports": [
                    "react"
                  ],
                  "locals": [
                    "module"
                  ]
                }
              ]
            }
          ]
        ]
      },
      "test": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joyfeel/webpack-hmr-template.git"
  },
  "keywords": [],
  "author": "Joey Chen <joybee210@gmail.com> (https://github.com/joyfeel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joyfeel/webpack-hmr-template/issues"
  },
  "homepage": "https://github.com/joyfeel/webpack-hmr-template#readme",
  "devDependencies": {
    "babel-core": "6.6.5",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "react-transform-hmr": "1.0.2",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "react": "0.14.7",
    "react-dom": "0.14.7"
  }
}
